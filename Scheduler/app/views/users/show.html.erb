<p id="notice"><%= notice %></p>

<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>
<% if logged_in? %>
<p>
  <strong>Timeslot:</strong>
  <%= @user.timeslot %>
</p>
<% end %>

<% if logged_in? %>
  <p>
  <strong>Link:</strong>
  <%= @user.link %>
  </p>
<% end %>




<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Logout', logout_path, method: :delete %>
