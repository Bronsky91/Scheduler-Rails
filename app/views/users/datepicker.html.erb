<div class="container">
  <div class="jumbotron">
    <h3 id="title">Select Appointment Date/Time:</h3>
    <input type="text" id="datep" />
    <div class="col-md-5" id="datepicker">
    </div>
    <div class="row">
      <div class="col-md-2" id="calInput">
        <h4>Subject</h4>
        <input id="subject">
        </input>
        <h4>Details</h4>
        <textarea id="details"></textarea>
      </div>
      <div class="col-md-3" id="emailDiv">
        <h4>Email Address</h3>
          <%= text_field_tag "email", "", id: "email"%>
          <%= button_to "Send Invite & Schedule Appointment", email_path, class: "btn btn-primary", id: "scheduleAct" %>
      </div>
    </div>
  </div>
</div>